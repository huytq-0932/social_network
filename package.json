    "body-parser": "^1.19.0",
    "cherio": "^1.0.0-rc.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "form-urlencoded": "^4.2.1",
    "js-base64": "^2.6.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "module-alias": "^2.2.2",
    "next": "9.4.0",
