  "devDependencies": {
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.38",
    "@zeit/next-less": "^1.0.1",
    "less": "^3.11.3",
    "next-compose-plugins": "^2.2.0",
    "nodemon": "^2.0.4",
    "redux-devtools-extension": "^2.13.8",
    "tsconfig-paths": "^3.9.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "server",
      "config",
      "routes"
    ],
    "exec": "yarn dev",
    "ext": "ts, json"
  },
  "_moduleAliases": {
    "@root": "./dist",
    "@core": "./dist/server/libs/core",
    "@libs": "./dist/server/libs",
    "@app": "./dist/server/app",
    "@config": "./dist/config",
    "@src": "./src"
  }
