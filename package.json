  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev-client": "cross-env MODE=dev-client nodemon -w routes -w public/static/locales",
    "dev-server": "cross-env MODE=dev-server nodemon",
    "dev": "cross-env IS_TS_NODE=true ts-node -r tsconfig-paths/register --project tsconfig.server.json server/index.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:next && npm run build:server",
    "start": "cross-env NODE_ENV=production node dist/server/index.js"
  },
  "keywords": [],
