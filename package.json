    "scenejs": "^1.1.7",
    "swr": "^0.2.3",
    "tough-cookie": "^4.0.0",
    "ts-node": "^8.10.2",
    "typescript": "3.8.3",
    "universal-cookie": "^4.0.3",
    "url-pattern": "^1.0.3",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "xlsx": "^0.16.5"
